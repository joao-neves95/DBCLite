<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>1.0.0.2</Version>
    <Authors>João Pedro Martins Neves (_SHIVAYL_)</Authors>
    <Company>João Pedro Martins Neves (_SHIVAYL_)</Company>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <RepositoryType>Git</RepositoryType>
    <Copyright>João Pedro Martins Neves</Copyright>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageId>DBCLite.PostgreSQL</PackageId>
    <Product>DBCLite.PostgreSQL</Product>
    <AssemblyName>DBCLite.PostgreSQL</AssemblyName>
    <RootNamespace>DBCLite.PostgreSQL</RootNamespace>
    <PackageProjectUrl>https://github.com/joao-neves95/DBCLite</PackageProjectUrl>
    <RepositoryUrl>https://github.com/joao-neves95/DBCLite</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Npgsql" Version="5.0.1.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DBCLite\DBCLite.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
